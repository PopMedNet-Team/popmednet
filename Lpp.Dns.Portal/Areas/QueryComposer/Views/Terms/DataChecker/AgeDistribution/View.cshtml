@using Lpp.QueryComposer

<script type="text/html" id="v_@ModelTermsFactory.DC_AgeDistribution">
    <br />
    <span class="bold center" style="margin-left:45%;" data-bind="visible:$index() > 0">And</span>
    <div class="row">
        <div class="col-sm-2 bold">Age Distribution:</div>
        &nbsp;
        <div class="col-sm-10">
            <span data-bind="foreach: Values.AgeDistributionValue">
                <span data-bind="visible: ($index() > 0)"> or </span>
                <span data-bind="visible: $data == 0, text: '&#60;0 years'"></span>
                <span data-bind="visible: $data == 1, text: '0-1 years'"></span>
                <span data-bind="visible: $data == 2, text: '2-4 years'"></span>
                <span data-bind="visible: $data == 3, text: '5-9 years'"></span>
                <span data-bind="visible: $data == 4, text: '10-14 years'"></span>
                <span data-bind="visible: $data == 5, text: '15-18 years'"></span>
                <span data-bind="visible: $data == 6, text: '19-21 years'"></span>
                <span data-bind="visible: $data == 7, text: '22-44 years'"></span>
                <span data-bind="visible: $data == 8, text: '45-64 years'"></span>
                <span data-bind="visible: $data == 9, text: '65-74 years'"></span>
                <span data-bind="visible: $data == 10, text: '75-110 years'"></span>
                <span data-bind="visible: $data == 11, text: '&#62;110 years'"></span>
                <span data-bind="visible: $data == 12, text: 'NULL or missing'"></span>
                <span data-bind="visible: $data == 13, text: 'Values outside of CDM specifications'"></span>
            </span>
            <span data-bind="visible: Values.AgeDistributionValue.length == 0">&lt;None Selected&gt;</span>
        </div>
    </div>
</script>
