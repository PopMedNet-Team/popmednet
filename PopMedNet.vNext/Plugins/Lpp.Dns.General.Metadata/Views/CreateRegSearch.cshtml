@model Lpp.Dns.General.Metadata.Models.MetadataRegSearchModel
<input type="hidden" id="hData" name="Data" /> <!--The entire model is stored in here as json-->
<div class="ui-groupbox" id="registrySearch">
    <div class="ui-groupbox-header"><span>Search Criteria</span></div>

    <fieldset class="ui-groupbox">
        <legend></legend>
        <div class="ui-groupbox-header"><span>Registry Items</span></div>
        <ol>
            <li>
