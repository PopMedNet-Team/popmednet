<script src="@this.Resource("CreateRegSearch.js")"></script>
<script>
    //Load the data here from the json that is serialized in from the model.
    var data = JSON.parse('@Html.Raw(Model.Data ?? "{}")');
    MetadataQuery.CreateRegistrySearch.init(data, $("#hData"), $("#registrySearch"));
</script>
