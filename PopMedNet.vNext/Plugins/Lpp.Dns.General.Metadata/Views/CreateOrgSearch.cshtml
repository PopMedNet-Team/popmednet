            </li>
            <li class="col3">
                <label for="chkDataModelPcori">
                    <input type="checkbox" id="chkDataModelPcori" data-bind="checked: DataModelPCORI" />
                    PCORnet CDM
                </label>
            </li>
            <li class="col3">
                <label for="chkDataModelOther">
                    <input type="checkbox" id="chkDataModelOther" data-bind="checked: DataModelOther" />
                    Other
                </label>
            </li>
        </ol>
    </fieldset>
</div>

<script src="@this.Resource("CreateOrgSearch.js")"></script>
<script>
    var data = JSON.parse('@Html.Raw(Model.Data ?? "{}")');
    MetadataQuery.CreateOrgSearch.init(data, $("#hData"), $("#orgSearch"));
</script>