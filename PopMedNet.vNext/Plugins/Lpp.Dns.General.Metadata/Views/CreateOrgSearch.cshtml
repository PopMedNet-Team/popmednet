@model Lpp.Dns.General.Metadata.Models.MetadataOrgSearchModel
 
<div class="ui-groupbox" id="orgSearch">
    <div class="ui-groupbox-header"><span>Search Criteria</span></div>
    <input type="hidden" id="hData" name="Data" />
    <fieldset>
        <legend>Organization Details</legend>
        <ol>
            <li>
                <label for="txtName">Name</label>
                <input type="text" id="txtName" data-bind="value: Name" style="width: 100%;" />
            </li>
            <li>
                <label for="txtHealthPlanSystemDescription">Organization Description (Contains Keyword or Phrase)</label>
                <input type="text" id="txtHealthPlanSystemDescription" data-bind="value: HealthPlanSystemDescription" style="width: 100%;" />
            </li>
        </ol>
    </fieldset>
    <fieldset class="ui-groupbox">
        <legend></legend>
        <div class="ui-groupbox-header"><span>Willing to Participate In:</span></div>
        <ol>
            <li class="col3">
                <label for="chkObservationalResearch">
                    <input type="checkbox" id="chkObservationalResearch" data-bind="checked: ObservationalResearch" />
                    Observational Research
                </label>
            </li>
            <li class="col3">
                <label for="chkClinicalTrials">
                    <input type="checkbox" id="chkClinicalTrials" data-bind="checked: ClinicalTrials" />
                    Clinical Trials
                </label>
            </li>
            <li class="col3">
                <label for="chkPragamaticClincialTrials">
                    <input type="checkbox" id="chkPragamaticClincialTrials" data-bind="checked: PragamaticClincialTrials" />
                    Pragmatic Clinical Trials
                </label>
            </li>
        </ol>
    </fieldset>
    <fieldset class="ui-groupbox">
        <legend></legend>
        <div class="ui-groupbox-header"><span>Types of Data Collected By Organization</span></div>
        <ol>
            <li class="col3">
                <label for="chkNone">
                    <input type="checkbox" id="chkNone" data-bind="checked: None" />
                    None
                </label>
            </li>
            <li class="col3">
                <label for="chkInpatient">
                    <input type="checkbox" id="chkInpatient" data-bind="checked: Inpatient" />
                    Inpatient
                </label>
            </li>
            <li class="col3">
                <label for="chkOutpatient">
                    <input type="checkbox" id="chkOutpatient" data-bind="checked: Outpatient" />
                    Outpatient
                </label>
            </li>
            <li class="col3">
                <label for="chkPharmacyDispensings">
                    <input type="checkbox" id="chkPharmacyDispensings" data-bind="checked: PharmacyDispensings" />
                    Pharmacy Dispensings
                </label>
            </li>
            <li class="col3">
                <label for="chkEnrollment">
                    <input type="checkbox" id="chkEnrollment" data-bind="checked: Enrollment" />
                    Enrollment
                </label>
            </li>
            <li class="col3">
                <label for="chkDemographics">
                    <input type="checkbox" id="chkDemographics" data-bind="checked: Demographics" />
