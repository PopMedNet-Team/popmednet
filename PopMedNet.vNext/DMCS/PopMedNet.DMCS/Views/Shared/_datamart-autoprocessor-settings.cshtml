<script type="text/x-template" id="datamart-autoprocessor-settings">
    <div id="crdAutoProcessorSetting" class="card mb-3">
        <div class="card-header">Auto Processor Settings</div>
        <div class="card-body">
            <p class="alert alert-info">Note: setting uploads results by automatically setting low cell counts to zero.</p>
            <div class="form-check mb-3">
                <input class="form-check-input" type="radio" name="chkAutoProcessor" id="chkNone" :value="0" v-model="myAutoProcess">
                <label class="form-check-label" for="chkNone">None</label>
            </div>
            <div class="form-check mb-3">
                <input class="form-check-input" type="radio" name="chkAutoProcessor" id="chkNotify" :value="1" v-model="myAutoProcess">
                <label class="form-check-label" for="chkNotify">Notify me of new queries, but do not process them automatically</label>
            </div>
            <div class="form-check mb-3">
                <input class="form-check-input" type="radio" name="chkAutoProcessor" id="chkRun" :value="2" v-model="myAutoProcess">
                <label class="form-check-label" for="chkRun">Process new queries automatically but do not upload results</label>
            </div>
            <div class="form-check mb-3">
                <input class="form-check-input" type="radio" name="chkAutoProcessor" id="chkUpload" :value="3" v-model="myAutoProcess">
                <label class="form-check-label" for="chkUpload">Process new queries automatically and upload results</label>
            </div>
        </div>
    </div>
</script>
