
@{
    ViewBag.Title = "Criteria Group Templates";
    Layout = "~/Views/_Layout.cshtml";
}
@section Head{
    <script src="~/js/Templates/Index.js"></script>
}

<article class="panel panel-default" data-bind="css: {visible: true}">
    <div class="panel-heading">
        <ol class="breadcrumb">
            <li><a href="/">Home</a></li>
            <li class="active"><a href="/templates">Criteria Group Templates</a></li>
        </ol>
    </div>
    <div class="panel-body">
        <div id="gTemplates" style="min-height: 400px;" data-bind="pmnGrid: {
            dataSource: ds,
            autoBind: false,
            savedSetting:{
                Key: 'Templates.Index.gTemplates.User:',
                Setting: dsSetting !== null ? dsSetting.Setting : null
            },
            scrollable: false,
            pageable: false,
            columns: [
                {field: 'Name', title: 'Name', template: Templates.Index.NameAchor}
            ]
             }">
        </div>
    </div>
    <nav class="panel-footer right">
        &nbsp;
    </nav>
</article>

