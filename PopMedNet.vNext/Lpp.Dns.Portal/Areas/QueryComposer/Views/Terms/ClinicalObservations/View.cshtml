@using Lpp.QueryComposer
<script type="text/html" id="v_@ModelTermsFactory.ClinicalObservationsID">
    <p class="bold center operator" data-bind="text:Global.Helpers.GetEnumString(Dns.Enums.QueryComposerOperatorsTranslation, Operator), visible:$index() > 0"></p>
    <div class="row row-noborder">
        <div class="col-sm-3 bold">Clinical Observations</div>
        <div class="col-sm-3" style="text-align:right;">
            <strong>Code Set:</strong><br />
            <strong>Search Method:</strong><br />
            <strong>Selected Codes:</strong><br />
            <strong>Result Range:</strong><br />
            <strong>Qualitative Result:</strong><br />
            <strong>Result Modifier:</strong><br/>
            <strong>Result Unit:</strong>
        </div>
        <div class="col-sm-6">
            <span data-bind="text:Global.Helpers.GetEnumString(Dns.Enums.ClinicalObservationsCodeSetTranslation, Values.CodeSet)"></span><br />
            "<span data-bind="text:Global.Helpers.GetEnumString(Dns.Enums.TextSearchMethodTypeTranslation, Values.SearchMethodType || Dns.Enums.TextSearchMethodType.ExactMatch)"></span>"<br />
            <span data-bind="text:Plugins.Requests.QueryBuilder.View.PrepareCollectionForDisplay((Values.CodeValues || ''), ';')"></span><br />
            Min:<span data-bind="text: Values.ResultRangeMin"></span>&nbsp; Max:<span data-bind="text: Values.ResultRangeMax"></span><br />
            <span data-bind="text:Values.QualitativeResult"></span><br />
            <span data-bind="text:Global.Helpers.GetEnumString(Dns.Enums.LOINCResultModifierTypeTranslation, Values.ResultModifier)"></span><br />
            <span data-bind="text:Values.ResultUnit"></span>
        </div>
    </div>
</script>