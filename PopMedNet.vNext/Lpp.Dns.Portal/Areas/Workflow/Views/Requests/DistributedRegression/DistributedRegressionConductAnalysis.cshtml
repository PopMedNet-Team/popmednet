<script src="~/areas/workflow/js/requests/DistributedRegression/ConductAnalysis.js"></script>
<!-- ko stopBinding:true -->
<section id="DRConductAnalysis" style="margin-top:15px;display:none;" data-bind="fadeVisible:true">

    <article class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">Analysis Center</h4>
        </div>
        <table class="panel-body table table-striped table-bordered table-hover">
            <colgroup>
                <col style="width: 33%;" />
                <col style="width: 33%;" />
                <col style="width: 33%;" />
            </colgroup>
            <thead>
                <tr>
                    <th>DataMart</th>
                    <th>Status</th>
                    <th>Message</th>
                </tr>
            </thead>
            <tbody data-bind="visible:AnalysisCenters.length == 0">
                <tr>
                    <td colspan="5" style="text-align:center;color:lightgray;">No Analysis Centers</td>
                </tr>
            </tbody>
            <tbody data-bind="foreach: AnalysisCenters">
                <tr>
                    <td><i class="k-icon k-i-plus-sm" data-bind="click:$root.OpenChildDetail.bind($data, ID),attr: {ID: 'img-' + ID}, visible: Children().length > 0"></i><a href="#" data-bind="text: Name"></a></td>
                    <td data-bind="text: Global.Helpers.GetEnumString(Dns.Enums.RoutingStatusTranslation, Status)"></td>
                    <td data-bind="html: Messages "></td>
                </tr>
                <tr data-bind="attr: {ID: 'response-' + ID}, visible:false">
                    <td colspan="3">
                        <div>
                            <table class="panel-body table table-stripped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>Name</th>
                                        <th>Submitted On</th>
                                        <th>Responded On</th>
                                    </tr>
                                </thead>
                                <tbody data-bind="foreach: Children">
                                    <tr>
                                        <td colspan="1"><a data-bind="click:$root.ViewChildResponse.bind($data, ID), text: Name" /></td>
                                        <td colspan="1" data-bind="text:moment(SubmittedOn).format('M/D/YYYY h:mm:ss A')"></td>
                                        <td colspan="1" data-bind="text:moment(ResponseTime).format('M/D/YYYY h:mm:ss A ')"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </article>
    <br class="clearfix" />

    <article class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">Data Partner Completed Routings</h4>
        </div>
        <table class="panel-body table table-striped table-bordered table-hover">
            <colgroup>
                <col style="width: 33%;" />
                <col style="width: 33%;" />
                <col style="width: 33%;" />
            </colgroup>
            <thead>
                <tr>
                    <th>DataMart</th>
                    <th>Status</th>
                    <th>Message</th>
                </tr>
            </thead>
            <tbody data-bind="visible:VirtualRoutings.length == 0">
                <tr>
                    <td colspan="5" style="text-align:center;color:lightgray;">No Completed Routings</td>
                </tr>
            </tbody>
            <tbody data-bind="foreach: VirtualRoutings">
                <tr>
                    <td><i class="k-icon k-i-plus-sm" data-bind="click:$root.OpenChildDetail.bind($data, ID),attr: {ID: 'img-' + ID}, visible: Children().length > 0"></i><a href="#" data-bind="text: Name"></a></td>
                    <td data-bind="text: Global.Helpers.GetEnumString(Dns.Enums.RoutingStatusTranslation, Status)"></td>
                    <td data-bind="html: Messages "></td>
                </tr>
                <tr data-bind="attr: {ID: 'response-' + ID}, visible:false">
                    <td colspan="3">
                        <div>
                            <table class="panel-body table table-stripped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>Name</th>
                                        <th>Submitted On</th>
                                        <th>Responded On</th>
                                    </tr>
                                </thead>
                                <tbody data-bind="foreach: Children">
                                    <tr>
                                        <td colspan="1"><a data-bind="click:$root.ViewChildResponse.bind($data, ID), text: Name" /></td>
                                        <td colspan="1" data-bind="text:moment(SubmittedOn).format('M/D/YYYY h:mm:ss A')"></td>
                                        <td colspan="1" data-bind="text:moment(ResponseTime).format('M/D/YYYY h:mm:ss A ')"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </article>
</section>
<!-- /ko -->