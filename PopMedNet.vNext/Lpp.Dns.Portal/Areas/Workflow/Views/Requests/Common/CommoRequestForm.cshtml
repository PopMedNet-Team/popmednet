                <colgroup>
                    <col style="width: 400px;" />
                    <col style="width: 200px; background-color: silver; font-style: italic;" />
                    <col style="width: 200px;" />
                    <col style="width: 200px;" />
                    <col style="width: 200px;" />
                    <col style="width: 200px;" />
