                    <col style="width: 200px;" />
                    <col style="width: 200px;" />
                </colgroup>
                <thead>
                    <tr>
                        <th>&nbsp;</th>
                        <th>Example</th>
                        <th>Group 1</th>
                        <th>Group 2</th>
                        <th>Group 3</th>
